        <TextView
            android:id="@+id/textView7"
            android:layout_width="87dp"
            android:layout_height="24dp"
            android:fontFamily="@font/poppins_medium"
            android:text="Weight"
            android:textColor="#FFFFFF"
            android:textSize="15dp"
