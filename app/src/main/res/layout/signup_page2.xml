<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/secondaryblue">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="388dp"
        android:layout_height="218dp"
        app:srcCompat="@drawable/group_12"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="349dp"
        android:layout_height="73dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Before we can get started, tell us a bit about yourself! "
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="21dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="244dp" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="94dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-16dp"
        tools:layout_editor_absoluteY="328dp">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="36dp"
            android:layout_height="34dp"
            app:srcCompat="@drawable/person_black_24dp_2"
            tools:layout_editor_absoluteX="36dp"
            tools:layout_editor_absoluteY="38dp" />

        <EditText
            android:id="@+id/editFeet"
            android:layout_width="84dp"
            android:layout_height="47dp"
            android:backgroundTint="@color/secondaryblue"
            android:ems="30"
            android:fontFamily="@font/poppins_medium"
            android:inputType="textPersonName"
            android:text="0"
            android:textColor="#FFFFFF"
            android:textSize="20dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="90dp"
            tools:layout_editor_absoluteY="37dp" />

        <EditText
            android:id="@+id/editInches"
            android:layout_width="109dp"
            android:layout_height="50dp"
            android:backgroundTint="@color/secondaryblue"
            android:ems="30"
            android:fontFamily="@font/poppins_medium"
            android:inputType="textPersonName"
            android:text="0"
            android:textColor="#FFFFFF"
            android:textSize="20dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="245dp"
            tools:layout_editor_absoluteY="37dp" />

        <View
            android:layout_width="140dp"
            android:layout_height="1dp"
            android:background="@android:color/white"
            tools:layout_editor_absoluteX="36dp"
            tools:layout_editor_absoluteY="87dp" />

        <View
            android:layout_width="120dp"
            android:layout_height="1dp"
            android:background="@android:color/white"
            tools:layout_editor_absoluteX="245dp"
            tools:layout_editor_absoluteY="88dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="67dp"
            android:layout_height="21dp"
            android:fontFamily="@font/poppins_medium"
            android:text="Feet"
            android:textColor="#FFFFFF"
            android:textSize="13dp"
            tools:layout_editor_absoluteX="90dp"
            tools:layout_editor_absoluteY="17dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="87dp"
            android:layout_height="24dp"
            android:fontFamily="@font/poppins_medium"
            android:text="Height"
            android:textColor="#FFFFFF"
            android:textSize="15dp"
            tools:layout_editor_absoluteX="36dp"
            tools:layout_editor_absoluteY="-4dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="68dp"
            android:layout_height="19dp"
            android:fontFamily="@font/poppins_medium"
            android:text="Inches"
            android:textColor="#FFFFFF"
            android:textSize="13dp"
            tools:layout_editor_absoluteX="245dp"
            tools:layout_editor_absoluteY="17dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="94dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="425dp">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="36dp"
            android:layout_height="34dp"
            app:srcCompat="@drawable/person_black_24dp_2"
            tools:layout_editor_absoluteX="36dp"
            tools:layout_editor_absoluteY="44dp" />

        <EditText
            android:id="@+id/editWeight"
            android:layout_width="288dp"
            android:layout_height="49dp"
            android:backgroundTint="@color/secondaryblue"
            android:ems="30"
            android:fontFamily="@font/poppins_medium"
            android:inputType="textPersonName"
            android:text="Weight"
            android:textColor="#FFFFFF"
            android:textSize="20dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="91dp"
            tools:layout_editor_absoluteY="33dp" />

        <View
